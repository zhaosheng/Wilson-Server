<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

  <servlet>
    <servlet-name>CallServlet</servlet-name>
    <servlet-class>servlet.CallServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CallServlet</servlet-name>
    <url-pattern>/call</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>WriteServlet</servlet-name>
    <servlet-class>servlet.WriteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WriteServlet</servlet-name>
    <url-pattern>/write</url-pattern>
  </servlet-mapping>
</web-app>
